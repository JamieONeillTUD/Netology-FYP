# Copy this file to `.env` and adjust the values as needed.
# The backend requires these PostgreSQL credentials to establish a connection.
DB_HOST=db        # Hostname of the PostgreSQL server (use "db" when running via docker-compose.dev.yml)
DB_PORT=5432      # TCP port exposed by the PostgreSQL server
DB_NAME=netology  # Name of the database to connect to
DB_USER=netology  # Database user with access to DB_NAME
DB_PASSWORD=netology  # Password for DB_USER
